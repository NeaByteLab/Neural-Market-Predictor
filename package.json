{
  "name": "neural-market-predictor",
  "version": "1.0.0",
  "description": "Neural network market predictor with real-time cryptocurrency data",
  "main": "dist/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "check": "npm run lint && npm run format:check"
  },
  "keywords": [
    "neural-network",
    "trading",
    "prediction",
    "typescript",
    "ai"
  ],
  "author": "NeaByteLab",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/NeaByteLab/Neural-Market-Predictor"
  },
  "homepage": "https://github.com/NeaByteLab/Neural-Market-Predictor#readme",
  "bugs": {
    "url": "https://github.com/NeaByteLab/Neural-Market-Predictor/issues"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-sonarjs": "^3.0.4",
    "jest": "^30.0.5",
    "prettier": "^3.6.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "ccxt": "^4.4.97",
    "mathjs": "^14.6.0"
  }
}
